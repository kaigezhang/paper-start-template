{
  "name": "react-template",
  "version": "1.0.0",
  "description": "react template for all my project",
  "scripts": {
    "clean:build": "rimraf build",
    "build:html": "node ./scripts/buildHtml.js",
    "prestart": "npm run clean:build",
    "start": "webpack-dashboard -c cyan -- node ./scripts/development.js",
    "start:log": "node ./scripts/development.js",
    "prebuild": "npm run clean:build",
    "build": "cross-env NODE_ENV=production node ./scripts/production.js",
    "analyse": "node ./scripts/analyse.js",
    "analyse:graph": "webpack-bundle-analyzer ./reports/stats.json",
    "postbuild": "npm run build:html",
    "build:serve": "http-server build/client -p 3000 --cors -o -c10",
    "tunnel": "lt --port 3000",
    "deploy": "node ./scripts/deploy.js"
  },
  "keywords": [
    "react"
  ],
  "author": "kaige",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.3",
    "icepick": "^2.3.0",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-popover": "^0.5.4",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.5.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.1",
    "bundle-loader": "^0.5.5",
    "cheerio": "^1.0.0-rc.2",
    "child-process-promise": "^2.2.1",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "colors": "^1.1.2",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^5.0.0",
    "happypack": "^4.0.1",
    "html-webpack-plugin": "^2.30.1",
    "json-server": "^0.12.1",
    "lodash": "^4.17.4",
    "mockjs": "^1.0.1-beta3",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^3.0.0-beta.7",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.5.9",
